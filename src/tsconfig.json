{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "moduleResolution": "node",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "sourceMap": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "lib": [
      "es2015", "es5", "dom"
    ],
    "types": [
      "node"
    ],
    "suppressImplicitAnyIndexErrors": true,
    "watch": true,
    "removeComments": true
  },
  "exclude": [
      "node_modules",
      "typings",
      "wwwroot/lib"
  ],
  "compileOnSave": true
}